{
  "grammars": [
    {
      "name": "perl",
      "camelcase": "Perl",
      "scope": "source.pm",
      "path": ".",
      "file-types": [
        "pm",
        "pl"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "(perl|pl)",
      "first-line-regex": "#!.*\\bperl\\b"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "ISC",
    "description": "A tree-sitter parser, for Perl!",
    "authors": [
      {
        "name": "veesh"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-perl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}